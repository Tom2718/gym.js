{
  "name": "gym.js",
  "version": "0.0.1",
  "description": "A JavaScript Environment for Reinforcement Learning.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "start": "webpack --watch & live-server --port=8085",
    "demo": "webpack"
  },
  "keywords": [
    "reinforcement learning"
  ],
  "author": "Thomas Tumiel",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.5.0",
    "live-server": "^1.2.1",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@tensorflow/tfjs": "^1.0.3",
    "phaser": "^3.16.2"
  }
}
